package com.Fatema.Autowired_Practice;

import org.springframework.stereotype.Component;

@Component
public class Bike implements Vehicle{
	
	private Tyre tyre;
	
	public Tyre getTyre() {
		return tyre;
	}

	public void setTyre(Tyre tyre) {
		this.tyre = tyre;
	}

	public void drive() {
		System.out.println("Bike"+" "+tyre);
	}
}
